{
  "name": "snowe",
  "version": "0.8.0",
  "manifest_version": 2,
  "description": "DKSS project tool for ServiceNow",
  "icons": {
    "512": "logo512.png"
  },
  "browser_action": {
    "default_icon": "static/icons/s.png",
    "default_popup": "index.html"
  },
  "permissions": [
    "activeTab",
    "storage"
  ],
  "content_scripts": [
    {
      "matches": ["https://*.service-now.com/*"],
      "js": [
        "static/js/content.chunk.js",
        "static/js/runtime-content.js"
      ]
    }
  ]
}
